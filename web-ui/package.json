    "@types/react-resizable": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "canvas": "^3.2.0",
    "cross-env": "^10.0.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-mock-extended": "^4.0.0-beta1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "ts-jest": "^29.3.4",
    "undici": "^7.16.0",
    "web-streams-polyfill": "^4.2.0"
  },
  "overrides": {
    "uri-js": "npm:uri-js-replace",
    "psl": "npm:@imput/psl@2.0.4",
    "data-urls@3.*": {
      "whatwg-url": "12.0.1"
    },
    "jest-environment-jsdom": {
      "jsdom": {
        "whatwg-url": "12.0.1"
      }
    },
    "@textea-json-viewer@4.0.1": {
      "@mui/material": "npm:@mui-material-replace@7.3.6",
      "react": "npm:react@19.2.0",
      "react-dom": "npm:react-dom@19.2.0"
    }
  },
  "resolutions": {
    "axios": ">=1.8.2",
    "esbuild": ">=0.25.0",
    "@textea/json-viewer/**/@mui/material": ">=7.3.6",
    "@textea/json-viewer/**/react": ">=19.2.0",
    "@textea/json-viewer/**/react-dom": ">=19.2.0"
  }
}
