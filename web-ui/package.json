{
  "name": "compliance-theater",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "turbo run dev",
    "debug": "turbo run debug",
    "build": "turbo run build",
    "build:typescript": "turbo run build:typescript",
    "build:publish": "yarn workspace compliance-theater install --immutable && turbo run build:publish",
    "build:clean": "turbo run build:clean",
    "test": "turbo run test --coverage",
    "test:fast": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "turbo": "^2.8.3"
  },
  "resolutions": {
    "lodash-es@npm:^4.17.21": "npm:4.17.23",
    "@esbuild-kit/core-utils/esbuild": "0.25.0"
  }
}
