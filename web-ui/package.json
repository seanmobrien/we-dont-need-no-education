    "mcp-handler": "^1.0.2",
    "mui-markdown": "^2.0.1",
    "next": "^15.5.9",
    "next-auth": "^5.0.0-beta.30",
    "node-html-parser": "^7.0.1",
    "openai": "^5.10.2",
    "openai-chat-tokens": "^0.2.8",
    "openid-client": "^6.8.0",
    "pdf-parse": "^1.1.1",
    "pino": "^9.6.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.5",
    "prexit": "^2.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-draggable": "^4.5.0",
    "react-error-boundary": "^6.0.0",
    "react-json-tree": "^0.20.0",
    "react-resizable": "^3.0.5",
    "redis": "^4.6.13",
    "semanticencoding": "^0.0.2",
    "tough-cookie": "^6.0.0",
    "ts-node": "^10.9.2",
    "ts-pattern": "^5.7.1",
    "tslib": ">= 1.0.0",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "vite": "^6.4.1",
    "zod": "^3.25.76",
    "zodex": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.0.0",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query-devtools": "^5.80.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^24.0.0",
