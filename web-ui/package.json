{
  "name": "compliance-theater",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "packageManagerConfig": {
    "ignore-peerdeps": [
      "@textea/json-viewer@4.0.1:@mui/material",
      "@textea/json-viewer@4.0.1:react",
      "@textea/json-viewer@4.0.1:react-dom"
    ]
  },
  "engines": {
    "node": "^24.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use yarn instead of npm')\"",
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=3096' next build",
    "drizzle-generate": "drizzle-kit generate --config=drizzle.config.ts",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "test:serial": "jest --runInBand",
    "test:concurrency-stress": "jest --maxWorkers=8 --maxConcurrency=10",
    "test:e2e": "playwright test --project=chromium-safe --project=firefox-safe",
    "test:e2e:safe": "playwright test --project=chromium-safe --project=firefox-safe --project=webkit-safe",
    "test:e2e:all": "cross-env RUN_ALL_TESTS=true playwright test",
    "test:e2e:mutation": "cross-env ENABLE_DATA_MUTATION_TESTS=true playwright test --project=chromium-mutation --project=firefox-mutation",
    "test:e2e:headed": "playwright test --headed --project=chromium-safe",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug --project=chromium-safe",
    "test:e2e:chromium": "playwright test --project=chromium-safe",
    "test:e2e:firefox": "playwright test --project=firefox-safe",
    "test:e2e:webkit": "playwright test --project=webkit-safe",
    "test:e2e:report": "playwright show-report",
    "lint": "next lint",
    "vercel-build": "cross-env NODE_OPTIONS='--max-old-space-size=3096' next build",
    "build:slow": "cross-env NODE_OPTIONS='--max-old-space-size=2048' timeout 1800 next build",
    "test:timeout": "timeout 600 jest",
    "test:long": "jest --testTimeout=60000"
  },
  "dependencies": {
    "@ai-sdk/azure": "^2.0.20",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/mcp": "^0.0.12",
    "@ai-sdk/openai": "^2.0.28",
    "@ai-sdk/react": "^2.0.23",
    "@auth/core": "0.41.1",
    "@auth/drizzle-adapter": "^1.4.1",
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.31",
    "@azure/storage-blob": "^12.27.0",
    "@badgateway/oauth2-client": "^3.3.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@googleapis/gmail": "^12.0.0",
    "@keycloak/keycloak-admin-client": "^26.3.4",
    "@microsoft/applicationinsights-clickanalytics-js": "^3.3.9",
    "@microsoft/applicationinsights-react-js": "^19.3.6",
    "@microsoft/applicationinsights-web": "^3.3.9",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/system": "^7.3.6",
    "@mui/x-data-grid-pro": "^8.3.1",
    "@mui/x-license": "^8.4.0",
    "@next/bundle-analyzer": "^15.4.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/api-logs": "0.57.2",
    "@opentelemetry/core": "1.30.1",
    "@opentelemetry/exporter-jaeger": "1.30.1",
    "@opentelemetry/instrumentation": "<0.57.2",
    "@opentelemetry/instrumentation-pino": "^0.48.0",
    "@opentelemetry/instrumentation-undici": "0.10.1",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-logs": "0.57.2",
