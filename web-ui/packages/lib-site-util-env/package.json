{
  "name": "@repo/lib-site-util-env",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./_common": {
      "types": "./src/_common.ts",
      "import": "./dist/_common.mjs",
      "require": "./dist/_common.js"
    },
    "./_client": {
      "types": "./src/_client.ts",
      "import": "./dist/_client.mjs",
      "require": "./dist/_client.js"
    },
    "./_server": {
      "types": "./src/_server.ts",
      "import": "./dist/_server.mjs",
      "require": "./dist/_server.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/_common.ts src/_client.ts src/_server.ts --format cjs,esm --external @repo/app --external compliance-theater",
    "dev": "tsup src/index.ts src/_common.ts src/_client.ts src/_server.ts --format cjs,esm --watch --external @repo/app --external compliance-theater",
    "test": "jest",
    "lint": "eslint src/"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5"
  }
}
