{
  "name": "@compliance-theater/typescript",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript utility types and helpers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./abortable-promise": {
      "types": "./dist/abortable-promise.d.ts",
      "default": "./dist/abortable-promise.js"
    },
    "./dual-key-map": {
      "types": "./dist/dual-key-map.d.ts",
      "default": "./dist/dual-key-map.js"
    },
    "./zod-to-json-structure": {
      "types": "./dist/zod-to-json-structure.d.ts",
      "default": "./dist/zod-to-json-structure.js"
    },
    "./generics": {
      "types": "./dist/generics.d.ts",
      "default": "./dist/generics.js"
    },
    "./guards": {
      "types": "./dist/guards.d.ts",
      "default": "./dist/guards.js"
    },
    "./record-decorators": {
      "types": "./dist/record-decorators.d.ts",
      "default": "./dist/record-decorators.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./singleton-provider": {
      "types": "./dist/singleton-provider/index.d.ts",
      "default": "./dist/singleton-provider/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "test": "jest",
    "lint": "echo 'Linting lib-typescript...'"
  },
  "dependencies": {
    "@compliance-theater/logger": "0.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5",
    "jest": "^30.0.5",
    "ts-jest": "^29.3.4"
  }
}
