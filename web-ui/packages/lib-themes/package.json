{
  "name": "@compliance-theater/themes",
  "version": "0.1.0",
  "private": true,
  "description": "Theme and styling utilities for the Title IX Victim Advocacy Platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "default": "./dist/themes/index.js"
    },
    "./styles": {
      "types": "./dist/styles/index.d.ts",
      "default": "./dist/styles/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "build:typescript": "echo 'Building lib/themes typescript...' && tsc -b tsconfig.json && echo 'TypeScript build complete.' || (echo 'Typescript compile did not succeed' >&2; exit 1)",
    "test": "jest",
    "lint": "echo 'Linting lib-themes...'",
    "clean:build": "rimraf .next dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@compliance-theater/logger": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "jest": "^30.0.5",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.3.4",
    "typescript": "^5"
  },
  "peerDependencies": {
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
